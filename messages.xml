<tryton>
    <data grouped="1">
        <record model="ir.message" id="msg_process_already_exists">
            <field name="text">A process already exists for cost plan "%s".</field>
        </record>
        <record model="ir.message" id="msg_cannot_assign_process_to_product">
            <field name="text">Process "%(process)s" cannot be assigned to product "%(product)s" because no BOM relation exists.</field>
        </record>
    </data>
</tryton>
